import java.util.*;
class Solution {
    public int romanToInt(String a) {
        int count = 0;
        for (int i=0;i<a.length();i++)
        {
            if ( i + 1 < a.length() && a.charAt(i) == 'I' && a.charAt(i + 1) == 'V'){
                count = count + 4;
                i++;
            }
            else if ( i + 1 < a.length() && a.charAt(i) == 'I' && a.charAt(i + 1) == 'X'){
                count = count + 9;
                i++;
            }
            else if ( i + 1 < a.length() && a.charAt(i) == 'X' && a.charAt(i + 1) == 'L'){
                count = count + 40;
                i++;
            }
            else if( i + 1 < a.length() && a.charAt(i) == 'X' && a.charAt(i + 1) == 'C'){
                count = count + 90;
                i++;
            }
            else if ( i + 1 < a.length() && a.charAt(i) == 'C' && a.charAt(i + 1) == 'D'){
                count = count + 400;
                i++;
            }
            else if ( i + 1 < a.length() && a.charAt(i) == 'C' && a.charAt(i + 1) == 'M'){
                count = count + 900;
                i++;
            }
            else if (a.charAt(i) == 'I'){
                count = count + 1;
            }
            else if (a.charAt(i) == 'V'){
                count = count + 5;
            }
            else if (a.charAt(i) == 'X'){
                count = count + 10;
            }
            else if (a.charAt(i) == 'L'){
                count = count + 50;
            }
            else if (a.charAt(i) == 'C'){
                count = count + 100;
            }
            else if (a.charAt(i) == 'D'){
                count = count + 500;
            }
            else if (a.charAt(i) == 'M'){
                count = count + 1000;
            }
        }
        return count;
    }
   
}
